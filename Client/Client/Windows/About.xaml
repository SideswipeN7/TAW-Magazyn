<Window x:Class="Client.Windows.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client.Windows"
        mc:Ignorable="d"
        Title="O Aplikacji" Height="300" Width="300" WindowStyle="None" BorderThickness="1" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid Background="#FF8FB6AD" Margin="0,0,-55,-12">
        <Label x:Name="LblAboutOpis" Content="Label" HorizontalAlignment="Left" Margin="10,73,0,0" VerticalAlignment="Top" FontFamily="Segoe UI Black" FontSize="16" Height="181" Width="333" />
        <Button x:Name="BtnAboutOk" Content="Ok" HorizontalAlignment="Left" Margin="141,272,0,0" VerticalAlignment="Top" Width="75" Click="BtnAboutOk_Click" FontFamily="Segoe UI Black" />
        <Image x:Name="image" HorizontalAlignment="Left" Height="88" Margin="112,0,0,0" VerticalAlignment="Top" Width="121" Source="/Client;component/Media/Images/untitle2d.png" Stretch="Fill">
            <Image.OpacityMask>
                <ImageBrush ImageSource="/Client;component/Media/Images/untitle2d.png"/>
            </Image.OpacityMask>
        </Image>
    </Grid>
</Window>