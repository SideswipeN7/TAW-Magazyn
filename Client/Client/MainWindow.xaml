                                        <DataGrid.Columns>
                                            <DataGridTextColumn   Header="Produkt" Width="*" Binding="{Binding Produkt}"/>
                                            <DataGridTextColumn   Header="Cena" Width="*" Binding="{Binding Cena}"/>
