<Window x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TabControl x:Name="MainTab">
            <TabItem x:Name="TabState" Header="Stan Magazynu"/>
            <TabItem x:Name="TabTransactions" Header="Transakcje" />
            <TabItem x:Name="TabClients" Header="Klienci" />
            <TabItem x:Name="TabItems" Header="Artykuł" />
            <TabItem x:Name="TabCategory" Header="Kategorie" />
        </TabControl>
    </Grid>
</Window>